{
  "name": "@mrpollard/react-rss-podcast-player",
  "version": "1.1.0",
  "description": "This is a react component that provides a podcast player based on an RSS feed you provide it.",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "moment": "^2.29.1",
    "react": "^16.4.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "rss-parser": "^3.12.0",
    "webpack": "^5.34.0"
  },
  "scripts": {
    "test": "jest",
    "start": "webpack --watch",
    "build": "webpack"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "contributors": [
    "Richard Deane <titchimoto@gmail.com>",
    "Mark N Carpenter Jr <mark@mncjr.com> (https://mncarpenter.ninja)"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^5.2.4",
    "enzyme": "^3.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mcarpenterjr/react-rss-podcast-player.git"
  },
